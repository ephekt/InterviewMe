<h1>Listing user_answers</h1>

<table>
  <tr>
    <th>Answer</th>
    <th>Question</th>
    <th>Difficulty</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_answers.each do |user_answer| %>
  <tr>
    <td><%= user_answer.answer %></td>
    <td><%= user_answer.question_id %></td>
    <td><%= user_answer.difficulty %></td>
    <td><%= user_answer.email %></td>
    <td><%= link_to 'Show', user_answer %></td>
    <td><%= link_to 'Edit', edit_user_answer_path(user_answer) %></td>
    <td><%= link_to 'Destroy', user_answer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User answer', new_user_answer_path %>
